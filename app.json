{
    "name": "Vc-music-ub",
    "description": "Telegram UserBot for Video Chat.",
    "logo": "https://telegra.ph/file/98651249ad219fdaa8a6f.jpg",
    "keywords": [
        "Telegram",
        "MusicPlayer",
        "VideoPlayer"
    ],
    "repository": "https://github.com/iamdivyeshh/Vc-music-bot",
    "env": {
        "SESSION": {
            "description": "Pyrogram String Session. From https://t.me/Python_StringGen_bot",
            "required": true
        },
        "API_ID": {
            "description": "Your api id from my.telegram.org/apps",
            "required": true
        },
        "HNDLR": {
            "description": "Set handler",
            "required": false,
            "value": "/"
        },
        "API_HASH": {
            "description": "Your api hash from my.telegram.org/apps",
            "required": true
            "value" : "1258806974" 
        },
        "SUDO_USERS": {
            "description": "User ID list user counts as admin everywhere(separator using space)",
            "required": true
        }
    },
    "addons": [],
    "buildpacks": [
        {
            "url": "heroku/python"
        },
        {
            "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
        }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    },
    "stack": "container"
}
